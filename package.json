{
  "name": "screen-time-tracker-app",
  "version": "2.0.0",
  "description": "Screen Time Tracker App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . screen-time-tracker-app --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"IdleTime\"",
    "package-linux": "electron-packager . screen-time-tracker-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "bojansimoski",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^12.2.2",
    "electron-packager": "^15.1.0"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.10",
    "auto-launch": "^5.0.5",
    "axios": "^0.20.0",
    "chart.js": "^2.9.4",
    "chartjs-plugin-zoom": "^0.7.7",
    "date-and-time": "^0.14.1",
    "electron-util": "^0.14.2",
    "hammerjs": "^2.0.8"
  }
}
